//Faça um programa que calcule a área e o perímetro de um retângulo cuja base é igual a 9 e a altura igual a 5.

programa {
	funcao inicio() {
		inteiro perimetroRetangulo = 9+9+5+5
		inteiro areaRetangulo = 9*5
		
	escreva("A área do retangulo é: ", areaRetangulo, ".", "\n")
	escreva("O perimetro do retangulo é: ", perimetroRetangulo, ".")
	}
}
