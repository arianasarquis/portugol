//Faça um programa que dadas duas notas, 5 e 8, calcule a média e a soma e imprima na tela.

programa {
	funcao inicio() {
		inteiro nota1 = 5
		inteiro nota2 = 8
		
		real soma = nota1 + nota2
		real media = soma / 2
		
		escreva("A soma das notas é: ", soma, ".", "\n")
		escreva("A média das notas é: ", media, ".", "\n")
	}
}
		